	<body onload="javascript:setfocus();">
		<!-- Display company logo -->
		<div id="logo">
			<img src="/logo.gif" alt="HP - Company Logo" />
		</div>

		<div id="topAds">
			<!-- Put your ads here -->
			<div class="ads" style="position: relative; top: 10px;"><a href="http://www.hp.com/networking" target="_blank" title="Outcomes that matter."><img src="/ads.jpg" alt="Outcomes that matter." /></a></div>
		</div>

		<div id="main">
			<!-- Display error or warning messages -->
			<div id="message">
</div>

			<!-- Display login form -->
			<div class="boxed">
				<form action="/goform/HtmlLoginRequest" method="post" id="loginForm" name="loginForm">
					<input type="hidden" name="error_url" value="20159f03dbb51cb0418c0beeabacf72cbd8d1810,/index.asp" />
					<input type="hidden" name="success_url" value="023bbeb9a31191bcff62534f97b7c741af23e2a8,http://wifiaccess.cpe.fr:80/transport.asp" />
					<input type="hidden" name="original_url" value="34165a1839f473a23a8fb11194f4eb8e35ac690e,http://download.jboss.org/jbosstools/examples/project-examples-maven-4.1.1.GA.xml" />
					<input type="hidden" name="subscription_url" value="188fac5a2fd87fbbb136b844939359b57c596725,https://wifiaccess.cpe.fr:443/subscribe.asp" />
					<input type="hidden" name="valid_fields" value="access_type username password" />

					<input type="hidden" id="access_type" name="access_type" value="login" />



					<table width="100%" border="0"  cellspacing="0" cellpadding="4">



						<tr>
							<td>&nbsp;</td>
							<td>
								<table border="0" width="100%" cellspacing="0" cellpadding="4">
									<tr>
										<td align="right"><label for="username">Username:</label></td>
										<td><input type="text" name="username" maxlength="127" size="25" value=""/></td>
									</tr>
									<tr>
										<td align="right"><label for="password">Password:</label></td>
										<td><input type="password" name="password" maxlength="127" size="25" /></td>
									</tr>
								</table>
							</td>
						</tr>
					</table>

					<hr/>

					<table border="0" width="100%" cellspacing="0" cellpadding="4">
						<tr>
							<td>
							</td>
							<td align="right"><input type="submit" name="login" id="loginButton" value="Proceed" /></td>
						</tr>
					</table>
				</form>
			</div>

		</div>
	</body>
	<script type="text/javascript" language="JavaScript" src="/prototype.js"></script>
